{
  "version": "1.0",
  "defaultSchema": "GOVDATA",
  "schemas": [
    {
      "name": "GOVDATA",
      "type": "custom",
      "factory": "org.apache.calcite.adapter.govdata.GovDataSchemaFactory",
      "operand": {
        "comment": "Unified government data schema for DJIA companies analysis (2010-present)",
        "executionEngine": "DUCKDB",
        "storageProvider": "S3",
        "s3Config": {
          "bucket": "${GOVDATA_S3_BUCKET}",
          "region": "${AWS_REGION:us-east-1}",
          "accessKeyId": "${AWS_ACCESS_KEY_ID}",
          "secretAccessKey": "${AWS_SECRET_ACCESS_KEY}",
          "prefix": "govdata-production/"
        },
        "localCacheDirectory": "${GOVDATA_CACHE_DIR:/tmp/govdata-cache}",
        "localParquetDirectory": "${GOVDATA_PARQUET_DIR:/tmp/govdata-parquet}",
        "enabledSchemas": ["SEC", "ECON", "GEO"],
        "startYear": 2010,
        "endYear": 2024,
        "autoDownload": true,
        "sec": {
          "companies": "DJIA",
          "enabledForms": ["10-K", "10-Q", "8-K", "DEF 14A", "3", "4", "5"],
          "excludeForms": ["424B"],
          "downloadTimeout": 2147483647,
          "maxConcurrentDownloads": 5,
          "rateLimitDelayMs": 100
        },
        "econ": {
          "enabledSources": ["fred", "bls", "treasury", "bea"],
          "fredApiKey": "${FRED_API_KEY}",
          "blsApiKey": "${BLS_API_KEY}",
          "beaApiKey": "${BEA_API_KEY}",
          "customFredSeries": [
            "DGS10", "DGS2", "DGS30", "UNRATE", "PAYEMS", "CPIAUCSL", "GDPC1"
          ],
          "fredSeriesGroups": {
            "treasury_rates": {
              "tableName": "fred_treasury_rates",
              "series": ["DGS1MO", "DGS3MO", "DGS6MO", "DGS1", "DGS2", "DGS3", "DGS5", "DGS7", "DGS10", "DGS20", "DGS30"],
              "partitionStrategy": "YEAR_MATURITY",
              "partitionFields": ["year", "maturity"],
              "comment": "U.S. Treasury constant maturity rates for all standard maturities"
            },
            "employment_indicators": {
              "tableName": "fred_employment_indicators",
              "series": ["UNRATE", "PAYEMS", "CIVPART", "EMRATIO", "U6RATE"],
              "partitionStrategy": "YEAR",
              "partitionFields": ["year"],
              "comment": "Employment and labor force participation indicators"
            }
          }
        },
        "geo": {
          "enabledSources": ["tiger", "census"],
          "censusApiKey": "${CENSUS_API_KEY}",
          "geographicScope": "USA",
          "includeStates": true,
          "includeCounties": true,
          "includeZipCodes": true
        }
      }
    }
  ]
}