{
  "comment": "U.S. government geographic data including Census TIGER boundaries, HUD USPS ZIP code crosswalks, and demographic statistics. Enables spatial analysis, geographic aggregation, and crosswalk between ZIP codes, counties, census tracts, congressional districts, and metropolitan areas.",
  "partitionedTables": [
    {
      "name": "states",
      "pattern": "type=boundary/year=*/states.parquet",
      "comment": "U.S. state boundaries and attributes from TIGER/Line shapefiles. Includes state FIPS codes, names, areas, and geographic boundaries. Updated annually with official Census Bureau boundaries.",
      "columns": [
        {
          "name": "state_fips",
          "type": "string",
          "nullable": false,
          "comment": "2-digit FIPS code identifying the state (e.g., '06' for California)"
        },
        {
          "name": "state_code",
          "type": "string",
          "nullable": false,
          "comment": "Geographic identifier (GEOID) for the state, matches state_fips"
        },
        {
          "name": "state_name",
          "type": "string",
          "nullable": false,
          "comment": "Full state name (e.g., 'California')"
        },
        {
          "name": "state_abbr",
          "type": "string",
          "nullable": false,
          "comment": "2-letter postal abbreviation (e.g., 'CA')"
        },
        {
          "name": "land_area",
          "type": "double",
          "nullable": false,
          "comment": "Land area in square meters"
        },
        {
          "name": "water_area",
          "type": "double",
          "nullable": false,
          "comment": "Water area in square meters"
        },
        {
          "name": "geometry",
          "type": "string",
          "nullable": false,
          "comment": "WKT representation of state boundary polygon"
        }
      ],
      "download": {
        "enabled": true,
        "dataSource": "tiger",
        "baseUrl": "https://www2.census.gov/geo/tiger/TIGER{year}/STATE/",
        "filePattern": "tl_{year}_us_state.zip",
        "variables": {
          "type": "conceptualMapper",
          "source": "geo-variable-mappings.json"
        },
        "conversion": {
          "type": "shapefile_to_parquet",
          "method": "duckdb_spatial",
          "geometryColumn": "geometry"
        }
      },
      "partitions": {
        "columnDefinitions": [
          {
            "name": "type",
            "type": "VARCHAR"
          },
          {
            "name": "year",
            "type": "INTEGER"
          }
        ]
      }
    },
    {
      "name": "counties",
