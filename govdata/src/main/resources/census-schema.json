{
  "partitionedTables": [
    {
      "name": "acs_population",
      "pattern": "type=acs/year=*/acs_population.parquet",
      "comment": "American Community Survey population demographics including age, sex, race, and ethnicity breakdowns. Provides total population counts and detailed demographic distributions at various geographic levels from state to census tract."
    },
    {
      "name": "acs_demographics",
      "pattern": "type=acs/year=*/acs_demographics.parquet",
      "comment": "Detailed demographic characteristics including household composition, family structure, marital status, and birth/fertility data from the American Community Survey."
    },
    {
      "name": "acs_income",
      "pattern": "type=acs/year=*/acs_income.parquet",
      "comment": "Household and family income statistics from ACS including median household income, per capita income, income distribution by brackets, and poverty status. Essential for economic analysis and demographic profiling."
    },
    {
      "name": "acs_poverty",
      "pattern": "type=acs/year=*/acs_poverty.parquet",
      "comment": "Poverty statistics including population below poverty level, poverty rates by age groups, and ratio of income to poverty level. Crucial for social program planning and economic equity analysis."
    },
    {
      "name": "acs_employment",
      "pattern": "type=acs/year=*/acs_employment.parquet",
      "comment": "Employment status, labor force participation, unemployment rates, and work status by demographic groups. Includes occupation categories and industry of employment."
    },
    {
      "name": "acs_education",
      "pattern": "type=acs/year=*/acs_education.parquet",
      "comment": "Educational attainment levels from less than high school to graduate degrees. Includes school enrollment status and field of bachelor's degree for college graduates."
    },
    {
      "name": "acs_housing",
      "pattern": "type=acs/year=*/acs_housing.parquet",
      "comment": "Housing characteristics including occupancy status, tenure (owner/renter), housing value, rent costs, number of rooms, and year structure built. Critical for real estate and housing policy analysis."
    },
    {
      "name": "acs_housing_costs",
      "pattern": "type=acs/year=*/acs_housing_costs.parquet",
      "comment": "Detailed housing costs including median home values, median rent, housing cost burden (percentage of income spent on housing), and mortgage status."
    },
    {
      "name": "acs_commuting",
      "pattern": "type=acs/year=*/acs_commuting.parquet",
      "comment": "Commuting patterns including means of transportation to work, travel time to work, departure time, and vehicles available. Important for transportation planning and infrastructure analysis."
    },
    {
      "name": "acs_health_insurance",
      "pattern": "type=acs/year=*/acs_health_insurance.parquet",
      "comment": "Health insurance coverage status including types of coverage (private, public, Medicare, Medicaid) by age groups and employment status."
    },
    {
      "name": "acs_language",
      "pattern": "type=acs/year=*/acs_language.parquet",
      "comment": "Language spoken at home and English proficiency levels. Includes detailed language categories for population 5 years and over."
    },
    {
      "name": "acs_disability",
      "pattern": "type=acs/year=*/acs_disability.parquet",
      "comment": "Disability status by age group and type of disability including hearing, vision, cognitive, ambulatory, self-care, and independent living difficulties."
    },
    {
      "name": "acs_veterans",
      "pattern": "type=acs/year=*/acs_veterans.parquet",
      "comment": "Veteran status and characteristics including period of service, disability rating, and demographic profile of veteran population."
    },
    {
      "name": "acs_migration",
      "pattern": "type=acs/year=*/acs_migration.parquet",
      "comment": "Geographic mobility and migration patterns including residence 1 year ago, domestic migration flows, and foreign-born population statistics."
    },
    {
      "name": "acs_occupation",
      "pattern": "type=acs/year=*/acs_occupation.parquet",
      "comment": "Detailed occupation categories and industry of employment. Cross-tabulated with demographic characteristics for workforce analysis."
    },
    {
      "name": "decennial_population",
      "pattern": "type=decennial/year=*/decennial_population.parquet",
      "comment": "Decennial Census total population counts and basic demographics. Most accurate population count conducted every 10 years (2010, 2020, etc.)."
    },
    {
      "name": "decennial_demographics",
      "pattern": "type=decennial/year=*/decennial_demographics.parquet",
      "comment": "Detailed demographic characteristics from the Decennial Census including age, sex, race, ethnicity, and household relationships."
    },
    {
      "name": "decennial_housing",
      "pattern": "type=decennial/year=*/decennial_housing.parquet",
      "comment": "Housing unit counts and occupancy status from Decennial Census. Includes vacant units, seasonal units, and tenure information."
    },
    {
      "name": "economic_census",
      "pattern": "type=economic/year=*/economic_census.parquet",
      "comment": "Economic Census data conducted every 5 years providing detailed statistics on business establishments, employment, payroll, and sales by industry."
    },
    {
      "name": "county_business_patterns",
      "pattern": "type=economic/year=*/county_business_patterns.parquet",
      "comment": "Annual County Business Patterns data showing number of establishments, employment, and payroll by industry at county level."
    },
    {
      "name": "population_estimates",
      "pattern": "type=population/year=*/population_estimates.parquet",
      "comment": "Annual population estimates between decennial censuses. Includes components of population change (births, deaths, migration)."
    }
  ],
